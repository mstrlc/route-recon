{
    "manifest_version": 2,
    "name": "Strava More Maps",
    "version": "1.0",
    "description": "Adds additional map options to Strava.",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.strava.com/*"
            ],
            "js": [
                "map-modifier.js"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "constants.js",
        "strings.js",
        "inject.js",
        "panorama.js",
        "assets/mapycom/*",
        "assets/osm/*",
        "icons/*"
    ]
}